import my.edu.utem.ftmk.dad.restorderapp.model.OrderType;
import my.edu.utem.ftmk.orderdbapp.model.ProductType;

@Repository
	public interface OrderTypeRepository extends JpaRepository<ProductType, Long> {

	OrderType save(OrderType orderType);

	
	
}
